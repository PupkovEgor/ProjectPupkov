package Laba2;

public class PuzirSortirovka {
	public static int[] sort() {
		Arraysix lk = new Arraysix();
		 int[] a =lk.arr(); 
		  for (int i = 0; i < a.length; i++) {
		      for (int j = 0; j < a.length; j++) {
		          if (a[j] > a[i]) {
		             int aj = a[j];
		             int ai = a[i];
		             a[j] = ai;
		             a[i] = aj;
		          }
		      }
		  }
	      return a;
		}
}